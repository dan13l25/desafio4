<div>
   <h1>{{mensaje}}</h1>
   <input id="productInput">
   <p id="message">Lista creada </p>
</div>

<ul id="productList">
    {{#each products}}
        <li>
            <h2>{{this.title}}</h2>
            <p>ID: {{this.id}}</p>
            <p>Description: {{this.description}}</p>
            <p>Price: {{this.price}}</p>
            <p>thumbnail: {{this.thumbnail}}</p>
            <p>code: {{this.code}}</p>
            <p>stock: {{this.stock}}</p>
            <p>status: {{this.status}}</p>
            <p>category: {{this.category}}</p>
            <button class="deleteButton" data-productname="{{this.name}}">Eliminar</button>
        </li>
    {{/each}}
</ul>

<script src="/socket.io/socket.io.js"></script>
<script src="/js/realTimeProducts.js"></script>